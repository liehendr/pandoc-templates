<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="$lang$" xml:lang="$lang$"$if(dir)$ dir="$dir$"$endif$>
<head>
  <meta charset="utf-8" />
  <meta name="generator" content="pandoc" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes" />
$for(author-meta)$
  <meta name="author" content="$author-meta$" />
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$" />
$endif$
$if(keywords)$
  <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$" />
$endif$
$if(description-meta)$
  <meta name="description" content="$description-meta$" />
$endif$
  <title>$if(title-prefix)$$title-prefix$ â€“ $endif$$pagetitle$</title>
  <style>
    $styles.html()$
  </style>
$for(css)$
  <link rel="stylesheet" href="$css$" />
<link rel="stylesheet" href="/share/vanilla.css">
<!-- font-aweesome -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<!-- font-awesome ends -->
<link rel="apple-touch-icon" sizes="180x180" href="/icons/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="/icons/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/icons/favicon-16x16.png">
<link rel="manifest" href="/icons/site.webmanifest">
<link rel="mask-icon" href="/icons/safari-pinned-tab.svg" color="#5bbad5">
<link rel="shortcut icon" href="/icons/favicon.ico">
<meta name="msapplication-TileColor" content="#2b5797">
<meta name="msapplication-config" content="/icons/browserconfig.xml">
<meta name="theme-color" content="#000000">
$endfor$
$for(header-includes)$
  $header-includes$
$endfor$
$if(math)$
$if(mathjax)$
  <script src="https://polyfill.io/v3/polyfill.min.js?features=es6"></script>
$endif$
  $math$
$endif$
  <!--[if lt IE 9]>
    <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv-printshiv.min.js"></script>
  <![endif]-->
</head>
<body>
  <nav class="nav-home">
    <div class="nav-container" id="navContainer">
        <img class="nav-logo banner" src="/share/liecorp-logo.png" alt="Logo Liecorp" height="50px">
        <!-- navbar content -->
        <div class="top-navbar" id="topNavbar"></div>
        <!-- navbar content end -->
    </div>
</nav>
$for(include-before)$
$include-before$
$endfor$
<div class="content-container">
  <div class="panel-left">
    <div class="content content-box">
      <!-- content area -->
$if(title)$
<header id="title-block-header">
<h1 class="title">$title$</h1>
$if(subtitle)$
<p class="subtitle">$subtitle$</p>
$endif$
$for(author)$
<p class="author">$author$</p>
$endfor$
$if(date)$
<p class="date">$date$</p>
$endif$
$if(abstract)$
<div class="abstract">
<div class="abstract-title">$abstract-title$</div>
$abstract$
</div>
$endif$
</header>
$endif$
$if(toc)$
<nav id="$idprefix$TOC" role="doc-toc">
$if(toc-title)$
<h2 id="$idprefix$toc-title">$toc-title$</h2>
$endif$
$table-of-contents$
</nav>
$endif$
$body$
    <!-- content area ends -->
    </div>
  </div>
</div>
            <!-- news section start -->
            <div class="content">
              <!-- news section head start -->
              <h3 class="latest-news latest-news-head">
                  <a title="Browse the news archives" href="/news/">Latest News</a>
                  <div class="triangle-down"></div>
              </h3>
              <!-- news section head end -->

              <!-- news section object start -->
              <div id="newsItems">Enable javascript to see news contents.</div>
              <!-- news section object end -->

              <!-- old news section head start -->
              <h3 class="latest-news latest-news-head">
                  <a title="Browse the news archives" href="/news/">Older News</a>
                  <div class="triangle-down"></div>
              </h3>
              <!-- news section head end -->

              <!-- news section object start -->
              <div>
                  <dl class="news-list" id="oldNewsItems">
                      <dt>No item</dt>
                      <dd>
                          Enable javascript to see content.
                      </dd>
                  </dl>
              </div>
              <!-- old news section object end -->
          </div>
          <!-- news section end -->
      </div>
      <div class="panel-right">
          <!-- secondary content block -->
          <div id="infoPanel" class="content content-box">Enable javascript to see contents.</div>
          <!-- secondary content block end -->

          <!-- nginx welcome message -->
          <div id="nginxWelcome" class="content">Enable javascript to see contents.</div>
          <!-- nginx welcome message end -->
      </div>
  </div>
$for(include-after)$
$include-after$
$endfor$
    <!-- Javascripts -->
    <script src="/share/scripts.js"></script>
    <script src="/share/nav.js"></script>
    <script src="/share/index.js"></script>
</body>
</html>
